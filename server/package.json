{
  "name": "server",
  "version": "1.0.0",
  "repository": "https://github.com/mission-apprentissage/c-est-qui-le-pro.git",
  "private": false,
  "module": "src/index.js",
  "type": "module",
  "imports": {
    "#src/*.js": "./src/*.js",
    "#tests/*.js": "./tests/*.js",
    "#src/*.ts": "./src/*.ts",
    "#tests/*.ts": "./tests/*.ts"
  },
  "scripts": {
    "start": "migrate-mongo up && node dist/index.js",
    "dev": "TSUP_DEV=true tsup",
    "build": "tsup",
    "test": "ACCOMPAGNATEUR_LOG_LEVEL=fatal mocha --inline-diffs --exit tests/**/**/*-test.ts tests/**/*-test.ts tests/*-test.ts tests/**/**/*-test.js tests/**/*-test.js tests/*-test.js",
    "cli": "ACCOMPAGNATEUR_LOG_DESTINATIONS=stderr tsx src/cli.js",
    "coverage": "nyc --temp-dir .coverage/.nyc_output --report-dir .coverage --reporter=lcov --reporter=html yarn test",
    "lint": "eslint src/ tests/",
    "migrate-mongo": "migrate-mongo"
  },
  "dependencies": {
    "@turf/turf": "6.5.0",
    "async-retry": "1.3.3",
    "axios": "1.4.0",
    "body-parser": "1.20.0",
    "boom": "7.3.0",
    "bunyan": "1.8.15",
    "bunyan-slack": "0.0.10",
    "chalk": "5.3.0",
    "commander": "9.2.0",
    "csv-parse": "5.0.4",
    "deep-equal-in-any-order": "2.0.6",
    "dotenv": "16.0.0",
    "env-var": "7.1.1",
    "express": "4.18.1",
    "express-mongo-sanitize": "2.2.0",
    "fast-luhn": "2.0.0",
    "geotoolbox": "2.0.3",
    "iconv-lite": "0.6.3",
    "joi": "17.6.0",
    "kysely": "0.27.4",
    "lodash-es": "4.17.21",
    "luxon": "2.3.2",
    "migrate-mongo": "10.0.0",
    "mockdate": "3.0.5",
    "moment": "2.29.4",
    "moment-timezone": "0.5.43",
    "mongodb": "4.5.0",
    "node-cache": "5.1.2",
    "node-xml-stream": "1.0.2",
    "oleoduc": "0.8.0",
    "pg": "8.12.0",
    "pretty-ms": "7.0.1",
    "rate-limiter-flexible": "2.3.7",
    "remove-accents": "0.5.0",
    "resolve-package-path": "4.0.3",
    "round-half-even": "1.3.0",
    "stream-json": "1.7.4",
    "stream-to-array": "2.3.0",
    "transform-coordinates": "1.0.0",
    "tsup": "8.2.4",
    "tsx": "4.17.0",
    "typescript": "5.5.4",
    "unzipper": "0.10.14"
  },
  "engines": {
    "node": ">=18.20.2"
  },
  "prettier": {
    "printWidth": 120,
    "bracketSpacing": true,
    "trailingComma": "es5"
  },
  "devDependencies": {
    "@faker-js/faker": "8.4.1",
    "@types/node": "22.5.0",
    "axiosist": "0.10.0",
    "chai": "4.3.7",
    "chai-as-promised": "7.1.1",
    "chai-diff": "1.0.2",
    "chai-dom": "1.12.0",
    "chai-files": "1.4.0",
    "chai-html": "2.1.0",
    "chalk-legacy": "npm:chalk@4.1.2",
    "eslint": "7.1.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-import-resolver-custom-alias": "1.3.2",
    "eslint-plugin-import": "2.28.1",
    "eslint-plugin-mocha": "10.1.0",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "3.1.3",
    "lint-staged": "13.2.0",
    "mocha": "10.7.3",
    "mongodb-memory-server": "8.5.2",
    "nock": "13.3.0",
    "nodemon": "2.0.16",
    "nyc": "15.1.0",
    "prettier": "2.6.2"
  },
  "lint-staged": {
    "*.js": [
      "eslint --cache --fix",
      "prettier --write"
    ]
  }
}
